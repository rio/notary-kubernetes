{
        "server": {
                "http_addr": ":4443",
		"tls_key_file": "/certs/notary-server.key",
		"tls_cert_file": "/certs/notary-server.crt"
        },
        "trust_service": {
                "type": "remote",
                "hostname": "notarysigner",
                "port": "7899",
                "key_algorithm": "ecdsa",
		"tls_ca_file": "/certs/root-ca.crt"
        },
        "logging": {
                "level": "info"
        },
        "storage": {
                "backend": "mysql",
                "db_url": "server@tcp(mysql:3306)/notaryserver?parseTime=True"
        }
}
